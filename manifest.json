{
    "name": "Notepad V1.",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "",
    "offline_enabled": true,
    "background": {
     "scripts": [
         "js/background.js"
           ]
    },
    "icons": { "16": "images/noteIcon-16.png",
           "48": "images/noteIcon-48.png",
          "128": "images/noteIcon.png"
    },
    "browser_action": {
        "default_icon" : "images/noteIcon.png",
        "default_title": "",
        "default_popup": "index.html"
    },
    "permissions": [
        "background", "unlimitedStorage", "tabs"
    ],
    "web_accessible_resources": [
    "index.html","js/popup.js"
    ]
}
